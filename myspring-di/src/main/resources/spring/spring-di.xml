<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

	<bean id="hiredateObject" class="java.util.Date" /><!--配置了系统类 -->
	<bean id="deptObj" class="cn.unicorn.myspring.di.vo.Dept"
		p:deptno="10" p:dname="哲♂学部" p:enable="true" /><!--创建Dept实例 -->
	<bean id="emp" class="cn.unicorn.myspring.di.vo.Emp" p:empno="7369"
		p:ename="smith" p:salary="233.3" p:hiredate-ref="hiredateObject"
		p:dept-ref="deptObj" />

</beans>
